<adapter>
  <unique-id>activemq-solace-adapter</unique-id>
  <start-up-event-imp>com.adaptris.core.event.StandardAdapterStartUpEvent</start-up-event-imp>
  <heartbeat-event-imp>com.adaptris.core.HeartbeatEvent</heartbeat-event-imp>
  <shared-components>
    <connections>
    <!-- Solace connection -->
      <jms-connection>
        <unique-id>solace-jms-01</unique-id>
        <!-- PLEASE SET USER -->
        <user-name></user-name>
        <!-- PLEASE SET PASSWORD -->
        <password></password>
        <vendor-implementation class="basic-solace-implementation">
            <!-- PLEASE PUT THE BROKER URL -->
            <broker-url>VALUE</broker-url>
            <message-vpn>nram-poc-vpn</message-vpn>
        </vendor-implementation>
      </jms-connection>
      <!-- Active MQ Connection (Amazon MQ) -->
      <jms-connection>
        <unique-id>activemq-jms-01</unique-id>
        <!-- PLEASE SET USER -->
        <user-name></user-name>
        <!-- PLEASE SET PASSWORD -->
        <password></password>
        <vendor-implementation class="advanced-active-mq-implementation">
          <!-- PLEASE PUT THE BROKER URL -->
          <broker-url>VALUE</broker-url>
        </vendor-implementation>
      </jms-connection>
    </connections>
  </shared-components>
  <event-handler class="default-event-handler">
    <unique-id>DefaultEventHandler</unique-id>
    <connection class="null-connection">
      <unique-id>agitated-einstein</unique-id>
    </connection>
    <producer class="null-message-producer">
      <unique-id>romantic-bartik</unique-id>
    </producer>
  </event-handler>
  <message-error-handler class="null-processing-exception-handler">
    <unique-id>silly-dijkstra</unique-id>
  </message-error-handler>
  <failed-message-retrier class="no-retries">
    <unique-id>jolly-ptolemy</unique-id>
  </failed-message-retrier>
  <channel-list>
    <channel>
      <unique-id>channel1-solace-to-activemq</unique-id>
      <auto-start>true</auto-start>
      <consume-connection class="shared-connection">
          <lookup-name>solace-jms-01</lookup-name>
      </consume-connection>
      <produce-connection class="shared-connection">
          <lookup-name>activemq-jms-01</lookup-name>
      </produce-connection>
      <workflow-list>
          <!-- workflow -->
          <standard-workflow>
              <!-- Sol2MQ: Solace consumer -->
              <consumer class="jms-queue-consumer">
                  <unique-id>channel1-solace-out</unique-id>
                  <acknowledge-mode>CLIENT_ACKNOWLEDGE</acknowledge-mode>
                  <message-translator class="auto-convert-message-translator">
                      <jms-output-type>Text</jms-output-type>
                  </message-translator>
                  <queue>Q.DEV.QUEUE.1</queue>
              </consumer>
              <service-collection class="service-list">
                <unique-id>lonely-bell</unique-id>
                <services>
                  <exception-handling-service-wrapper>
                    <unique-id>exceptionWrapper-1</unique-id>
                    <service class="service-list">
                      <unique-id>exceptionTested</unique-id>
                      <services>
                        <log-message-service>
                          <unique-id>testing-gunzip-1</unique-id>
                          <log-level>DEBUG</log-level>
                          <log-prefix>1) Testing GUnzip</log-prefix>
                        </log-message-service>
                        <gunzip-service>
                          <unique-id>gunzip-service-1</unique-id>
                        </gunzip-service>
                        <log-message-service>
                          <unique-id>gunzip-passed-1</unique-id>
                          <log-level>DEBUG</log-level>
                          <log-prefix>2) GUnzip succeeded</log-prefix>
                        </log-message-service>
                      </services>
                    </service>
                    <exception-handling-service class="service-list">
                      <unique-id>exceptionThrown-1</unique-id>
                      <services>
                        <log-message-service>
                          <unique-id>gunzip-failed-1</unique-id>
                          <log-level>DEBUG</log-level>
                          <log-prefix>3) GUnzip failed</log-prefix>
                        </log-message-service>
                      </services>
                    </exception-handling-service>
                    <exception-message-metadata-key>adp.exception.wrapper.message</exception-message-metadata-key>
                  </exception-handling-service-wrapper>
                  <gzip-service>
                    <unique-id>cocky-engelbart</unique-id>
                  </gzip-service>
                </services>
              </service-collection>
              <!-- Sol2MQ: MQ publisher -->
              <producer class="jms-queue-producer">
                  <unique-id>channel1-mq-in</unique-id>
                  <acknowledge-mode>CLIENT_ACKNOWLEDGE</acknowledge-mode>
                  <message-translator class="text-message-translator">
                      <metadata-filter class="regex-metadata-filter">
                          <exclude-pattern>(JMS_IBM_Report_PAN|JMS_IBM_Last_Msg_In_Group|JMS_Solace_DeadMsgQueueEligible|JMS_Solace_ElidingEligible|JMS_Solace_DeliverToOne|JMS_Solace_isXML|message\.type)</exclude-pattern>
                      </metadata-filter>
                      <move-jms-headers>true</move-jms-headers>
                  </message-translator>
                  <delivery-mode>PERSISTENT</delivery-mode>
                  <session-factory class="jms-default-producer-session"/>
                  <queue>DEV.QUEUE.1</queue>
              </producer>
              <unique-id>channel1-solace-to-mq</unique-id>
              <message-metrics-interceptor>
                  <unique-id>channel1-solace-to-mq-MessageMetrics</unique-id>
                  <timeslice-duration>
                      <unit>MINUTES</unit>
                      <interval>5</interval>
                  </timeslice-duration>
                  <timeslice-history-count>12</timeslice-history-count>
              </message-metrics-interceptor>
              <in-flight-workflow-interceptor>
                  <unique-id>channel1-solace-to-mq-InFlight</unique-id>
              </in-flight-workflow-interceptor>
          </standard-workflow>
      </workflow-list>
    </channel>
    <!-- Channel 2: ActiveMQ (Amazon MQ) to Solace  -->
    <channel>
      <unique-id>channel2-activemq-to-solace</unique-id>
      <auto-start>true</auto-start>
      <consume-connection class="shared-connection">
          <lookup-name>activemq-jms-01</lookup-name>
      </consume-connection>
      <produce-connection class="shared-connection">
          <lookup-name>solace-jms-01</lookup-name>
      </produce-connection>
      <workflow-list>
          <!-- workflow -->
          <standard-workflow>
              <!-- MQ2Sol: MQ consumer -->
              <consumer class="jms-queue-consumer">
                  <unique-id>channel2-MQ-out</unique-id>
                  <acknowledge-mode>CLIENT_ACKNOWLEDGE</acknowledge-mode>
                  <message-translator class="auto-convert-message-translator">
                      <jms-output-type>Text</jms-output-type>
                  </message-translator>
                  <queue>DEV.QUEUE.2</queue>
              </consumer>
              <service-collection class="service-list">
                <unique-id>lonely-bell-2</unique-id>
                <services>
                  <exception-handling-service-wrapper>
                    <unique-id>exceptionWrapper-2</unique-id>
                    <service class="service-list">
                      <unique-id>exceptionTested</unique-id>
                      <services>
                        <log-message-service>
                          <unique-id>testing-gunzip-2</unique-id>
                          <log-level>DEBUG</log-level>
                          <log-prefix>1) Testing GUnzip</log-prefix>
                        </log-message-service>
                        <gunzip-service>
                          <unique-id>gunzip-service-2</unique-id>
                        </gunzip-service>
                        <log-message-service>
                          <unique-id>gunzip-passed-2</unique-id>
                          <log-level>DEBUG</log-level>
                          <log-prefix>2) GUnzip succeeded</log-prefix>
                        </log-message-service>
                      </services>
                    </service>
                    <exception-handling-service class="service-list">
                      <unique-id>exceptionThrown-2</unique-id>
                      <services>
                        <log-message-service>
                          <unique-id>gunzip-failed-2</unique-id>
                          <log-level>DEBUG</log-level>
                          <log-prefix>3) GUnzip failed</log-prefix>
                        </log-message-service>
                      </services>
                    </exception-handling-service>
                    <exception-message-metadata-key>adp.exception.wrapper.message</exception-message-metadata-key>
                  </exception-handling-service-wrapper>
                  <gzip-service>
                    <unique-id>silly-wing</unique-id>
                  </gzip-service>
                </services>
              </service-collection>
              <!-- MQ2Sol: Solace producer -->
              <producer class="jms-queue-producer">
                  <unique-id>channel2-solace-in</unique-id>
                  <acknowledge-mode>CLIENT_ACKNOWLEDGE</acknowledge-mode>
                  <message-translator class="text-message-translator">
                      <metadata-filter class="regex-metadata-filter">
                          <exclude-pattern>(JMS_IBM_Report_PAN|JMS_IBM_Last_Msg_In_Group|JMS_Solace_DeadMsgQueueEligible|JMS_Solace_ElidingEligible|JMS_Solace_DeliverToOne|JMS_Solace_isXML|message\.type)</exclude-pattern>
                      </metadata-filter>
                      <move-jms-headers>true</move-jms-headers>
                  </message-translator>
                  <delivery-mode>PERSISTENT</delivery-mode>
                  <session-factory class="jms-default-producer-session"/>
                  <queue>Q.DEV.QUEUE.2</queue>
              </producer>
              <unique-id>channel2-mq-to-solace</unique-id>
              <message-metrics-interceptor>
                  <unique-id>channel2-mq-to-solace-MessageMetrics</unique-id>
                  <timeslice-duration>
                      <unit>MINUTES</unit>
                      <interval>5</interval>
                  </timeslice-duration>
                  <timeslice-history-count>12</timeslice-history-count>
              </message-metrics-interceptor>
              <in-flight-workflow-interceptor>
                  <unique-id>channel2-mq-to-solace-InFlight</unique-id>
              </in-flight-workflow-interceptor>
          </standard-workflow>
      </workflow-list>
    </channel>
  </channel-list>
  <message-error-digester class="standard-message-error-digester">
    <unique-id>ErrorDigest</unique-id>
    <digest-max-size>100</digest-max-size>
  </message-error-digester>
</adapter>
